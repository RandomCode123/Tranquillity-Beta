{
    "normalIdentifier": [
        {
            "global": {
                "tokenType":"codeBlock",
                "hideSymbol": "True"
            },
            "{": {
                "rangeDirection": "left",
                "endSymbol": "}"
            },
            ";": {
                "rangeDirection": "left"
            }
        }, 
        {
            "global": {
                "tokenType": "sentence",
                "rangeDirection": "right"
            },
            "import ": {},
            "class ": {}, "func": {},
            "while ": {}, "if ": {}, "elif ": {}, "else ": {},
            "var ": {}, "return ": {}, "break ": {}
        },
        {
            "=": {
                "tokenType": "sentence",
                "rangeDirection": "bothSides"
            }
        },
        {
            "(": {
                "tokenType": "codeBlock",
                "endSymbol": ")",
                "hideSymbol": "True"
            }	
        },
        {
            "global":{
                "tokenType": "sentence",
                "rangeDirection": "bothSides"
            },
            " and ": {}, " or ": {}, 
            "!": { 
                "rangeDirection": "right" 
            },
            "==": {}, ">": {}, "<": {}, ">=": {}, 
            "<=": {}
        },
        {
            "global": {
                "tokenType": "sentence",
                "rangeDirection": "bothSides"
            },
            "<-": {}, "->": {}, " as ":{}
        },
        {
            "**": {
                "tokenType": "sentence",
                "rangeDirection": "bothSides"
            }
        },
        {
            "global": {
                "tokenType": "sentence",
                "rangeDirection": "bothSides"
            },
            "*": {}, "/": {}
        },
        {
            "global": {
                "tokenType": "sentence",
                "rangeDirection": "bothSides"
            },
            "+": {}, "-": {}
        }
    ],
    "additional": {
        "global": {
            "rangeDirection": "left",
            "hideSymbol": "True"
        },
        "}": {}, ")": {}
    }
}
